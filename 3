x = float(input("Введите значение x: "))
h = float(input("Введите значение h: "))
n = int(input("Введите значение n: "))

# Инициализация счетчика для хранения количества членов, удовлетворяющих условию
count = 0

# Проверяем удовлетворение условию для каждого члена последовательности
for i in range(n+1):
    cos_value = math.cos(x + i*h)
    if abs(cos_value) < 0.5:
        count += 1

# Выводим результат
print(f"Количество членов последовательности с cos < 0.5: count")
